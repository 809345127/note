{
	"name": "go-impl-to-intf",
	"displayName": "Go Implementation to Interface",
	"description": "Navigate from Go method implementation to its interface definition",
	"version": "0.0.1",
	"publisher": "go-dev-tools",
	"license": "MIT",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onLanguage:go",
		"onCommand:go-impl-to-intf.findInterface"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "go-impl-to-intf.findInterface",
				"title": "Find Interface Definition",
				"icon": "$(go-to-file)"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorLangId == 'go'",
					"command": "go-impl-to-intf.findInterface",
					"group": "navigation@1"
				}
			],
			"editor/title": [
				{
					"when": "editorLangId == 'go'",
					"command": "go-impl-to-intf.findInterface",
					"group": "navigation@1"
				}
			]
		},
		"keybindings": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.14.37",
		"@types/vscode": "^1.60.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
